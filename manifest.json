{
	"name": "LinkToId",
	"version": "0.1",
	"manifest_version": 2,
	"description": "This extension helps to identify and use linkable IDs within a page.",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["lib/jquery-1.11.1.min.js", "lib/linktoid.js"],
      "css": ["lib/linktoid.css"]
    }
  ]
}