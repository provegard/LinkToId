{
	"name" : "LinkToId",
	"version" : "0.0.1",
	"manifest_version" : 2,
	"short_name" : "LinkToId",
	"description" : "This extension helps to identify and use linkable IDs within a page.",
	"author" : "Per Rovegard",
	"content_scripts" : [{
			"matches" : ["<all_urls>"],
			"js" : ["linktoid.js"],
			"css" : ["linktoid.css"]
		}
	],
	"icons" : {
		"16" : "16.png",
		"32" : "32.png",
		"48" : "48.png",
		"128" : "128.png"
	}
}
